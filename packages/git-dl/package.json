{
  "name": "@nikelborm/git-dl",
  "version": "0.1.28",
  "type": "module",
  "description": "Download any directory at any depth from any repo on GitHub on any git ref",
  "license": "MIT",
  "scripts": {
    "start": "node --env-file=.env ./git-dl.ts",
    "scratchpad": "node --use-strict --env-file=.env ./scratchpad.ts",
    "test": "vitest --typecheck --watch",
    "format": "",
    "coverage": "vitest run --coverage",
    "test-once": "vitest --run",
    "test-jsr": "tspc && jsr publish --dry-run",
    "build": "./scripts/build.sh",
    "doc": "./scripts/generate_docs.sh",
    "docs": "./scripts/generate_docs.sh",
    "print-important-ts-files": "find . -type f -name '*.ts' |grep -vE '^./(node_modules|tmp|dist|(vite\\.config|.*\\.spec|scratchpad)\\.ts)'",
    "watch-docs": "find src/ cli.ts index.ts errors.ts ./scripts/generate_docs.sh | entr -can ./scripts/generate_docs.sh"
  },
  "dependencies": {
    "@effect/cli": "catalog:",
    "@effect/platform": "catalog:",
    "@effect/platform-node": "catalog:",
    "@octokit/core": "^7.0.6",
    "@octokit/request-error": "^7.1.0",
    "@octokit/types": "^16.0.0",
    "effect": "catalog:",
    "effect-errors": "catalog:",
    "outdent": "catalog:",
    "tar-fs": "^3.0.8",
    "tsafe": "catalog:",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@effect/language-service": "catalog:",
    "@effect/vitest": "catalog:",
    "@nikelborm/tsconfig": "workspace:*",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-json": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-terser": "catalog:",
    "@total-typescript/ts-reset": "catalog:",
    "@types/tar-fs": "^2.0.4",
    "@vitest/coverage-v8": "catalog:",
    "jsr": "catalog:",
    "rimraf": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-visualizer": "catalog:",
    "ts-namespace-import": "catalog:",
    "ts-patch": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nikelborm/effect-garden.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist-types/index.d.ts",
  "bin": {
    "git-dl": "dist/git-dl.js",
    "gdl": "dist/git-dl.js"
  },
  "bugs": {
    "url": "https://github.com/nikelborm/effect-garden/issues",
    "email": "evadev@duck.com"
  },
  "files": [
    "dist",
    "src",
    "!dist/minified",
    "!**/*.tsbuildinfo",
    "!**/*.spec.*",
    "!**/scratchpad.*",
    ".github/FUNDING.yml",
    "template.env",
    "cli.ts",
    "errors.ts",
    "index.ts"
  ],
  "homepage": "https://github.com/nikelborm/effect-garden#readme",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index": {
      "types": "./dist-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index.js": {
      "types": "./dist-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist-types/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./errors.js": {
      "types": "./dist-types/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./cli": {
      "types": "./dist-types/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./cli.js": {
      "types": "./dist-types/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "github",
    "git",
    "octokit",
    "github-api",
    "folder",
    "scraper",
    "downloader",
    "download",
    "extract",
    "tarball",
    "targz",
    "directory"
  ],
  "author": {
    "name": "nikelborm",
    "email": "evadev@duck.com",
    "url": "https://github.com/nikelborm"
  },
  "contributors": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ],
  "maintainers": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ]
}
