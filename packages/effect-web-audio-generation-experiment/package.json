{
  "name": "effect-web-audio-generate",
  "version": "0.1.0",
  "type": "module",
  "description": "Generates missing Effect wrappers for Web Audio API using @webref/idl",
  "license": "MIT",
  "scripts": {
    "build": "tspc",
    "prepack": "rm -rf dist dist-types && ./node_modules/.bin/tspc",
    "dev": "tspc --watch",
    "generate": "bun scripts/generate.ts",
    "generate:watch": "bun --watch scripts/generate.ts"
  },
  "files": [
    ".github/FUNDING.yml",
    "dist-types",
    "dist",
    "index.ts",
    "package.json",
    "src",
    "scripts",
    "!**/*.spec.*",
    "!**/*.tsbuildinfo",
    "!**/scratchpad.*"
  ],
  "peerDependencies": {
    "effect": "catalog:"
  },
  "dependencies": {
    "@webref/idl": "^3.72.1",
    "effect-web-audio": "workspace:*"
  },
  "homepage": "https://github.com/nikelborm/effect-garden/tree/main/packages/effect-web-audio-generate",
  "devDependencies": {
    "@effect/language-service": "catalog:",
    "@nikelborm/tsconfig": "workspace:*",
    "ts-namespace-import": "catalog:",
    "ts-patch": "catalog:",
    "typescript": "catalog:",
    "webidl2": "^24.5.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nikelborm/effect-garden.git",
    "directory": "packages/effect-web-audio-generation-experiment"
  },
  "bugs": {
    "url": "https://github.com/nikelborm/effect-garden/issues",
    "email": "evadev@duck.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.js": {
      "types": "./dist-types/src/*.d.ts",
      "default": "./dist/src/*.js"
    },
    "./*.ts": "./src/*.ts",
    "./*": {
      "types": "./dist-types/src/*.d.ts",
      "default": "./dist/src/*.js"
    },
    "./internal/*": null,
    "./package.json": "./package.json"
  },
  "author": {
    "name": "nikelborm",
    "email": "evadev@duck.com",
    "url": "https://github.com/nikelborm"
  },
  "contributors": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ],
  "maintainers": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ]
}
