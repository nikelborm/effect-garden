{
  "json.schemas": [
    {
      "fileMatch": ["*.tsconfig.json"],
      "url": "https://json.schemastore.org/tsconfig"
    }
  ],
  "stylelint.validate": [
    "css",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.words": [
    "addgroup",
    "Dont",
    "gefo",
    "incorr",
    "midimessage",
    "ondelete",
    "TMIDI",
    "trellisform",
    "tspc",
    "valltest",
    "VNAs",
    "Voidify",
    "yandexgpt",
    "айди",
    "инстанса",
    "нейронки",
    "Промпт",
    "промпта",
    "промптами",
    "Эндпоинт"
  ],
  "emojisense.languages": {
    "*": true
  },
  "ltex.checkFrequency": "edit",
  "ltex.enabled": [
    "bibtex",
    "context",
    "context.tex",
    "html",
    "latex",
    "markdown",
    "typescript",
    "javascript",
    "org",
    "restructuredtext",
    "rsweave"
  ],
  "yaml.schemas": {
    "file:///home/nikel/.vscode/extensions/atlassian.atlascode-3.0.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml",
    "https://raw.githubusercontent.com/compose-spec/compose-go/master/schema/compose-spec.json": "file:///./dev.compose.yaml"
  },
  "github-actions.workflows.pinned.workflows": [
    ".github/workflows/release.yml",
    ".github/workflows/test.yml"
  ],
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "typescript.preferences.importModuleSpecifierEnding": "js",
  "javascript.preferences.importModuleSpecifierEnding": "js",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "cmake.ignoreCMakeListsMissing": true,
  "typescript.tsdk": "./packages/tsconfig/node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
